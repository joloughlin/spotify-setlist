{
  "name": "react-starter-kit",
  "repository": "react-starter-kit",
  "version": "1.2.0",
  "engines": {
    "node": "8.10.0"
  },
  "description": "Intrepid starter kit for React with Redux applications",
  "main": "webpack.config.js",
  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config=config/webpack.development.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-production": "NODE_ENV=production node_modules/.bin/webpack --config=config/webpack.production.js",
    "build-staging": "NODE_ENV=staging node_modules/.bin/webpack --config=config/webpack.staging.js",
    "initial-setup": "rm -rf .git && git init && git add -A && git commit -m 'Initialize repository' && yarn install",
    "precommit": "lint-staged",
    "deploy-staging": "yarn build-staging && node scripts/deploy.js staging",
    "deploy-production": "yarn build-production && node scripts/deploy.js production",
    "list-builds": "node scripts/listBuilds",
    "activate-build": "node scripts/activateBuild"
  },
  "author": "David Rodriguez Fuentes <davidrf.developer@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "fetch-mock": "^5.9.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.8.1",
    "lint-staged": "^3.4.0",
    "moment": "^2.18.1",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.1",
    "react-page-object": "^1.0.11",
    "react-test-renderer": "^15.6.1",
    "redis": "^2.7.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-s3-plugin": "^1.0.0-rc.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "history": "^4.6.3",
    "humps": "^2.0.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "recompose": "^0.25.0",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote --trailing-comma all",
      "git add"
    ]
  }
}
